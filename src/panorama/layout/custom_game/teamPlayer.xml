<root>
    <styles>
        <include src="file://{resources}/styles/custom_game/game_setup.css" />
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/teamPlayer.js" />
    </scripts>
    <Panel class="teamPlayerContainer">
        <DOTAAvatarImage id="playerAvatar" />
        <DOTAHeroImage id="playerHeroImage" heroname="" />
        <Panel id="playerSkillContainer">
            <DOTAAbilityImage id="playerSkill1" abilityname="life_stealer_empty_1" onactivate="onPlayerHeroAbilityClicked(1)" />
            <DOTAAbilityImage id="playerSkill2" abilityname="life_stealer_empty_1" onactivate="onPlayerAbilityClicked(2)" />
            <DOTAAbilityImage id="playerSkill3" abilityname="life_stealer_empty_1" onactivate="onPlayerAbilityClicked(3)" />
            <DOTAAbilityImage id="playerSkill4" abilityname="life_stealer_empty_1" onactivate="onPlayerAbilityClicked(4)" />
            <DOTAAbilityImage id="playerSkill5" abilityname="life_stealer_empty_1" onactivate="onPlayerAbilityClicked(5)" />
            <DOTAAbilityImage id="playerSkill6" abilityname="life_stealer_empty_1" onactivate="onPlayerAbilityClicked(6)" />
        </Panel>
        <Panel class="playerNameContainer">
            <Label id="playerIsHostPanel" />
            <Label id="playerName" text="PLAYER_NAME" />
        </Panel>
    </Panel>
</root>
